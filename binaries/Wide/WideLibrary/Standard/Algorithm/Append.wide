module Standard {
    module Algorithm {
        Append(range2) {
            return function(range1)[range2 := Utility.Move(range2)] {
                return function()[first := true, range1 := Utility.Move(range1), range2 := Utility.Move(range2)] {
                    if (first) {
                        var := range1();
                        if (var) return Utility.Move(var);
                        first = false;
                    }
                    return range2();
                };
            };
        }
    }
}