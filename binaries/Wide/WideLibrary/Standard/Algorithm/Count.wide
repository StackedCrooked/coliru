module Standard {
    module Algorithms {
        Count() {
            return Count(function(val) { return true; });
        }
        Count(pred) {
            return function(range)[pred := move(pred)] {
                count := 0;
                range | Filter(move(pred)) | ForEach(function(val) { ++count; });
                return count;
            };
        }
    }
}