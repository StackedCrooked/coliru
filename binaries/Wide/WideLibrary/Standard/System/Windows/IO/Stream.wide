module Standard {
    module IO {
        using Windows := cpp("WideLibrary/test2.h");
        Out(val := uint8) {
            byteswritten := uint32();
            Windows.WriteConsoleA(
                Windows.GetStdout(),
                reinterpret_cast(void.pointer, &val),
                1,
                &byteswritten,
                null
            );
        }
    }
}