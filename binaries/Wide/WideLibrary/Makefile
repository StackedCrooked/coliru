ifndef config
  config=debug32
endif

ifndef verbose
  SILENT = @
endif

ifndef CC
  CC = gcc
endif

ifndef CXX
  CXX = g++
endif

ifndef AR
  AR = ar
endif

ifeq ($(config),debug32)
  OBJDIR     = ../Obj/WideLibrary/x32
  TARGETDIR  = ../Build/x86/Debug
  TARGET     = $(TARGETDIR)/libWideLibrary.a
  DEFINES   += -DTEAMCITY=225
  INCLUDES  += -I../..
  CPPFLAGS  += -MMD -MP $(DEFINES) $(INCLUDES)
  CFLAGS    += $(CPPFLAGS) $(ARCH) -g -m32 -std=c++11 -D __STDC_CONSTANT_MACROS -D __STDC_LIMIT_MACROS -fPIC
  CXXFLAGS  += $(CFLAGS) 
  LDFLAGS   += -m32 -L/usr/lib32
  LIBS      += 
  RESFLAGS  += $(DEFINES) $(INCLUDES) 
  LDDEPS    += 
  LINKCMD    = $(AR) -rcs $(TARGET) $(OBJECTS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
	@echo Running post-build commands
	mkdir -p ../Deployment/
	cp -r ../WideLibrary ../Deployment/
  endef
endif

