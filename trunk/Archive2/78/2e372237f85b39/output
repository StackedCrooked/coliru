














































int main() {
    int box1 = 30;
    int box2 = 99;
    int box3 = 140;
    int box4 = 200;
    int box5 = 220;
    int box6 = 260;
    int box7 = 300;
    int box8 = 310;
    int box9 = 500;
    int box10 = 100;

    std::cout << std::max({ box1 , box2 , box3 , box4 , box5 , box6 , box7 , box8 , box9 , box10});
}
