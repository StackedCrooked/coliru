void func( int a1 ) { int b1 = a1; } void func( int a1, int a2 ) { int b1 = a1, b2 = a2; } void func( int a1, int a2, int a3 ) { int b1 = a1, b2 = a2, b3 = a3; }
template <typename Arg1> void operator( )( Arg1 arg1 ) const; template <typename Arg1, typename Arg2> void operator( )( Arg1 arg1, Arg2 arg2 ) const; template <typename Arg1, typename Arg2, typename Arg3> void operator( )( Arg1 arg1, Arg2 arg2, Arg3 arg3 ) const;
