is_callable<decltype(object1), std::string>() = 0
is_callable<decltype(object2), std::string>() = 0
is_callable<decltype(lambda), std::string>() = 1
is_callable<decltype(lambda1), std::string>() = 0
is_callable<decltype(lambda1), std::string, int>() = 0
is_callable<decltype(func1), std::string>() = 1
is_callable<decltype(func1), std::string, int>() = 0
