check<A&, A>::value = [1]
check<const A&, const A>::value = [1]
check<volatile A&, volatile A>::value = [1]
check<volatile const A&, volatile const A>::value = [1]
check<A*&, A*>::value = [1]
check<const A*&, const A*>::value = [1]
check<volatile A*&, volatile A*>::value = [1]
check<volatile const A*&, volatile const A*>::value = [1]
