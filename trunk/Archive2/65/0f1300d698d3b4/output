Hellos World
SELECT distinct mp.msg_id, mp.msg_pattern, mp.msg_interpretation, mp.event_type  FROM message_patterns mp   join message_source ms on (mp.msg_id=ms.msg_id)   join message_category mc on (mp.msg_id=ms.msg_id)  where ms.invocation_id = 'abc' and mc.msg_category in ('ERROR'  ,'WARNING')
